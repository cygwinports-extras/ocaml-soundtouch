inherit ocaml utils

DESCRIPTION="OCaml MP3 ID3 tag library"
HOMEPAGE="http://savonet.sourceforge.net/wiki/OCamlLibs"
SRC_URI="mirror://sourceforge/savonet/${P}.tar.gz"

src_compile() {
	lndirs
	cd ${B}
	check_pkg_config SOUNDTOUCH soundtouch-1.0
	cygconf
	cygmake -j1 CLIBS="SoundTouch BPM stdc++"
}
